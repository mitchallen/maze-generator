{
  "name": "@mitchallen/maze-generator",
  "version": "0.1.18",
  "description": "maze generator",
  "main": "dist/maze-generator.js",
  "scripts": {
    "//": "To use: npm run test-nodemon (assumes global location of mocha)",
    "start": "node dist/maze-generator.js",
    "test": "mocha --recursive --timeout 20000",
    "test-nodemon": "nodemon /usr/local/bin/mocha --recursive --timeout 5000",
    "test-hexagon": "grunt && /usr/local/bin/mocha --recursive test/hexagon/ --timeout 5000",
    "test-square": "grunt && /usr/local/bin/mocha --recursive test/square/ --timeout 5000",
    "test-triangle": "grunt && /usr/local/bin/mocha --recursive test/triangle/ --timeout 5000",
    "test-circle": "grunt && /usr/local/bin/mocha --recursive test/circle/ --timeout 5000",
    "test-ascii": "grunt && /usr/local/bin/mocha --recursive test/ascii/ --timeout 5000",
    "test-debug": "grunt && /usr/local/bin/mocha -g '@DEBUG' --recursive --timeout 20000",
    "test-jenkins": "grunt && /usr/local/bin/mocha --recursive --timeout 20000 -R xunit test/ > test-reports.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mitchallen/maze-generator.git"
  },
  "author": "Mitch Allen <npm@mitchallen.com> (http://mitchallen.com)",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify-derequire": "^0.9.4",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-shell": "^1.3.0",
    "should": "^10.0.0",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "@mitchallen/connection-grid": "^0.1.25"
  }
}
