{
  "name": "@mitchallen/maze-generator",
  "version": "0.1.10",
  "description": "maze generator",
  "main": "index.js",
  "scripts": {
    "//": "To use: npm run test-nodemon (assumes global location of mocha)",
    "start": "node index.js",
    "test": "mocha --recursive --timeout 20000",
    "test-nodemon": "nodemon /usr/local/bin/mocha --recursive --timeout 5000",
    "test-hexagon": "nodemon /usr/local/bin/mocha --recursive test/hexagon/ --timeout 5000",
    "test-square": "nodemon /usr/local/bin/mocha --recursive test/square/ --timeout 5000",
    "test-ascii": "nodemon /usr/local/bin/mocha --recursive test/ascii/ --timeout 5000",
    "test-debug": "nodemon /usr/local/bin/mocha -g '@DEBUG' --recursive --timeout 20000",
    "test-jenkins": "mocha --recursive --timeout 20000 -R xunit test/ > test-reports.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mitchallen/maze-generator.git"
  },
  "author": "Mitch Allen <npm@mitchallen.com> (http://mitchallen.com)",
  "license": "ISC",
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-shell": "^1.3.0",
    "should": "^10.0.0",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "@mitchallen/connection-grid": "^0.1.14"
  }
}
